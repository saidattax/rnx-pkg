{
	"extends": "../../tsconfig.base",
	"compilerOptions": {
		"composite": true,
		"rootDir": "src",
		"outDir": "build/esm",
		"tsBuildInfoFile": "build/esm/index.tsbuildinfo",
		"lib": ["DOM", "ESNext"],
		"strict": false,
		"noImplicitReturns": false,
		"skipLibCheck": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"jsx": "react"
	},
	"include": ["src"],
	"references": [{ "path": "../notion-types" }, { "path": "../notion-utils" }]
}
